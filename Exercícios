resultado
                #Exercicio 10
leia lista_numeros
leia lista_pesos
var somatorio_numeros = 0
var somatorio_pesos = 0
para cada elemento da lista_numeros, faça:
  somatorio_numeros = somatorio_numeros + (elemento_atual * peso_correspondente)
para cada peso da lista_pesos,faça:
  somatorio_peso= somatoria_peso + peso
media = somatorio_numeros / somatorio pesos
